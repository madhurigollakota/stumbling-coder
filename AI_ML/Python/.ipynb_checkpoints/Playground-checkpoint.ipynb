    "def multiply(*args):\n",
    "    out=1\n",
    "    for arg in args:\n",
    "        out=out*arg\n",
    "    return out\n",
    "\n",
    "def apply(*args, operator):\n",
    "    if operator == '+':\n",
    "        return sum(args)\n",
    "    if operator == '*':\n",
    "        return multiply(*args)"
   ]
